{
  "name": "geekie-design-system",
  "version": "0.1.0",
  "description": "Geekie's design system",
  "author": "Geekie",
  "devDependencies": {
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-mdx-gfm": "^7.4.5",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-webpack5": "^7.0.26",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "chromatic": "^6.20.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "storybook": "^7.0.26",
    "storybook-design-token": "^3.0.0-beta.3",
    "style-dictionary": "^3.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.17.1"
  },
  "files": [
    "build/built-tokens/**",
    "build/built-presets/**",
    "build/helpers/**",
    "build/index.*",
    "build/token-presets/**",
    "build/tokens.*"
  ],
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "scripts": {
    "prepare": "yarn build-folders && yarn build-tokens && yarn build-presets && yarn tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "yarn build-tokens && storybook build",
    "build-folders": "mkdir -p build/less",
    "build-tokens": "ts-node ./tokens/config/style-dictionary-build.ts && cp ./built-tokens/less/_variables.less build/less/",
    "build-presets": "ts-node ./token-presets/config/buildPresets.ts && cp ./token-presets/less/_presets.less build/less/",
    "chromatic": "npx chromatic"
  }
}